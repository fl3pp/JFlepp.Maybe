<Project>

	<PropertyGroup>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>
		
		<RepositoryUrl>https://github.com/jflepp/JFlepp.Maybe</RepositoryUrl>
		<PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
		<PackageLicenseFile>license.txt</PackageLicenseFile>
		<Version>1.0.1</Version>
		<Authors>JFlepp</Authors>
		<Description>A Maybe type for C#, aimed as an idiomatic port of the option type in F# to C#</Description>
		<PackageIcon>Logo.PNG</PackageIcon>
	</PropertyGroup>

	<ItemGroup>
		<None Include="$(RootDirectory)$(PackageLicenseFile)" Pack="true" PackagePath="$(PackageLicenseFile)" Visible="false" />
		<None Include="$(RootDirectory)$(PackageIcon)"  Pack="true" PackagePath="$(PackageIcon)" Visible="false" />
	</ItemGroup>

</Project>
