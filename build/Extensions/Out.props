<Project>

	<PropertyGroup>
		<AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
	</PropertyGroup>

	<PropertyGroup>
		<OutDir>$(OutDirectory)bin\</OutDir>
		<OutDir Condition="'$(IsTest)'=='True'">$(OutDirectory)test\</OutDir>
		<BaseIntermediateOutputPath>$(OutDirectory)obj\$(MSBuildProjectName)\</BaseIntermediateOutputPath>
		<PackageOutputPath>$(OutDirectory)packages\</PackageOutputPath>
	</PropertyGroup>

</Project>
